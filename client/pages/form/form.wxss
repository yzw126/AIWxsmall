/* pages/form/form.wxss */
.swiper-tab {
  width: 100%;
  border-bottom: 1px solid #777;
  text-align: center;
  line-height: 40px;
  background-color: #fff;
}


.swiper-tab-list {
  font-size: 13px;
  display: inline-block;
  width: 25%;
  color: #777;
}
.on {
  color: #23af08;
  border-bottom: 10rpx solid #23af08
}

.swiper-box {
  width: 100%;
  overflow: hidden;
}


.swiper-box view {
   /* text-align: center;  */
}
.none{
  color: #23af08;
  position: absolute;
  top: 350rpx;
  left: 0;
  bottom: 0;
  right: 0;
}
.form{
  width: 90%;
  position: absolute;
  top: 0;
  background: #ffffff;
  margin-bottom: 20rpx;
  padding: 5%;
}
.form view{
  margin-bottom: 20rpx;
}

.swiper image {
  height: 100%;
  width: 100%;
}
.top{
  width: 80%;
  height: 100px;
  margin: 10px auto;
  padding: 5%;
  position: relative;
  color: #000;
  border: 1px solid #23af08;
  font-size: 40rpx;
}
.top .list{
  width: 100%;
  height: 800px;
}
.top .list .listn{
  width: 100%;
  height: 100px;
  margin-bottom:10px;
}
.top .list .listn .img{
  width: 100px;
  height: 100px;
  margin: 10px;
}
.top .list .listName{
  margin-left: 130px;
  margin-top: -80px;
}
.top .list .name{
  display: inline-block;
  position: relative;
  right:0;
  font-size: 50rpx;
}



